---
import { RiMailLine as MailIcon } from "react-icons/ri";
import { RiInstagramLine as InstagramLogo } from "react-icons/ri";

---
<footer>
    <MailIcon/>
    <InstagramLogo/>
</footer>
<style>
    footer {
        @apply py-[8vh] border-t-2 w-[80%] mx-auto flex justify-center gap-x-5; 
    }
    footer > svg {
        @apply size-8
    }
</style>